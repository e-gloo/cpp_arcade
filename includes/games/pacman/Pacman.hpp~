#ifndef PACMAN_HPP_
# define PACMAN_HPP_

# include "games/AGame.hpp"
# include "display/IDisplayManager.hpp"

class Pacman
{
public:
Nibbler();
  virtual				~Nibbler();
  virtual void				startGame(IDisplayManager &);
  virtual int				play(char);
  std::vector<t_snakePosition *>	*getSnake() const;
};

